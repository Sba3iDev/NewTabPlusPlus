{
    "manifest_version": 3,
    "name": "NewTab++",
    "version": "1.0.0",
    "description": "Enhance your New Tab page with customizable shortcuts",
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "src/newtab.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; style-src 'self'; img-src 'self' https: data:;"
    }
}
